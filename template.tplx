((*- extends 'classic.tplx' -*))

((*- block margins -*))
    \geometry{verbose,tmargin=1.25in,bmargin=1.25in,lmargin=1.25in,rmargin=1.25in}
((*- endblock margins -*))

((*- block style_prompt *))
    \newcommand{\prompt}[3]{
        \needspace{1.1cm}
        \noindent{\color{#2}#1 [#3]:}
    }
    ((* endblock style_prompt *))

((* set charlim = 94 *))
